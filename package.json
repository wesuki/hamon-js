{
  "name": "hamon",
  "version": "0.3.1",
  "description": "A zen puzzle game featuring stones and waves.",
  "main": "./index.html",
  "scripts": {
    "start": "parcel ./index.html --open",
    "prebuild-dev": "shx rm -rf dist/*",
    "build-dev": "parcel build ./index.html --no-minify --public-url ./",
    "prebuild": "shx rm -rf dist/*",
    "build": "parcel build ./index.html --public-url ./",
    "push-gh-pages": "push-dir --dir=dist --branch=gh-pages --cleanup --verbose"
  },
  "dependencies": {
    "parcel-bundler": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "parcel-plugin-git-build-info": "^0.0.1",
    "push-dir": "^0.4.1",
    "shx": "^0.3.2"
  },
  "resolutions": {
    "@babel/preset-env": "^7.13.8"
  },
  "keywords": [
    "javascript",
    "puzzle game"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wesuki/hamon-js.git"
  },
  "author": "wesuki",
  "bugs": {
    "url": "https://github.com/wesuki/hamon-js/issues"
  },
  "homepage": "https://github.com/wesuki/hamon-js#readme"
}
